{% extends 'base.html.twig' %}

{% block title %}Entrer une sortit{% endblock %}

{% block stylesheets %}

<link rel="stylesheet" type="text/css" href={{ asset('assets/vendor/bootstrap/css/bootstrap.min.css') }}>
<link rel="stylesheet" type="text/css" href={{ asset('assets/css/util.css') }}>
<link rel="stylesheet" type="text/css" href={{ asset('assets/css/form.css') }}>
<link rel="stylesheet" type="text/css" href={{ asset('assets/fonts/font-awesome-4.7.0/css/font-awesome.min.css') }}>
<link rel="stylesheet" type="text/css" href={{ asset('assets/fonts/iconic/css/material-design-iconic-font.min.css') }}>
<link rel="stylesheet" href="{{ asset('assets/css/profil.css') }}">
{% endblock %}

{% block body %}
 

<div class="container-contact100">
		<div class="wrap-contact100">
			{{ form_start(formWentOut, {'attr': {'class': 'contact100-form validate-form'}})}}
				<span class="contact100-form-title">
					Entrer une sortie
				</span>

				<div class="wrap-input100 validate-input bg1" >
					<span class="label-input100">Date</span>
					{{ form_widget(formWentOut.date, {'attr': {'class': 'input100'}}) }}
				</div>

				<div class="wrap-input100 validate-input bg1 rs1-wrap-input100" >
					<span class="label-input100">Km</span>
					{{ form_widget(formWentOut.km, {'attr': {'class': 'input100'}}) }}
				</div>

				<div class="wrap-input100 validate-input bg1 rs1-wrap-input100" >
					<span class="label-input100">Dur√©e</span>
					{{ form_widget(formWentOut.duration, {'attr': {'class': 'input100'}}) }}
				</div>

				<div class="container-contact100-form-btn">
					<button class="contact100-form-btn">
						<span>
							Entrer
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</button>
				</div>
			{{ form_end(formWentOut) }}
		</div>
	</div>



{% endblock %}

{% block javascripts %}

{% endblock %}


